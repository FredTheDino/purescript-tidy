module ZInfixOperators where

lt = 1 < 2
le = 1 <= 2
gt = 2 < 1
ge = 2 <= 1

ltChain = 1 < 2 && 3 < 4 || 5 < 6
leChain = 1 <= 2 && 3 <= 4 || 5 <= 6
gtChain = 2 < 1 && 4 < 3 || 6 < 5
geChain = 2 <= 1 && 4 <= 3 || 6 <= 5

dollarChain = pure $ b < a
dollarChain2 = pure $ b < a $ asdf

